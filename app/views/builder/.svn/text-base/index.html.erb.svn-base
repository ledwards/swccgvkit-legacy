<div class="header">
	<div id="notice_box">
		<%= image_tag '/images/ajax-loader.gif', :id => 'spinner', :style=>"display:none;" %>
		<p id="notice">Welcome, <%= @user.name %></p>
	</div>
	
	<a href id="logo">SWCCG vkit Online - Create custom virtual card cutouts for Star Wars CCG.</a>
	
	<%= render :partial => "search_pane" %>
	
	<p id = "links"><%= render :partial => "links" %></p>
</div>

<div class="left">
  <div class="saved_cardlists">
    <%= render(:partial => "cardlists", :object => @cardlists) %>
  </div>
</div>

<div class="right">
  <div class="cart" id="cart">
    <%= render(:partial => "cart", :object => @cart.line_items) %>
  </div>
</div>

<div class="center">
	<h1>Search Results</h1>	
	<div class="search-results" id ="search_results"></div>
</div>